<%= @q %>
<div class="box_left ">
  <%= render "books/list_menu" %>
</div>

<div class="box_center">
  <h4>Wyniki wyszukiwania:</h4>
  <div class="panel panel-default">
    <!-- Table -->
    <table class="table table-striped">
      <tr>
        <th></th>
        <th>Tytuł</th>
        <th>Autor</th>
        <th>Wydawnictwo</th>
        <th></th>
      </tr>
      <%@books.each do |book| %>
          <tr>
            <td><%= image_tag book.picture_file_name, class: "zdjecie_mini" %></td>
            <td><%=book.title %></td>
            <td><%=book.author %></td>
            <td><%=book.publishing %></td>
            <td><%= link_to "Szczegóły", book %></td>
          </tr>
      <% end %>
      </table>
  </div>
</div>
<div class="box_right">
  <%= render "books/show_last_books" %>
</div>